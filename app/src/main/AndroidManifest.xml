        <activity android:name="com.wyre.trade.cash.InfoActivity" />
        <activity android:name="com.wyre.trade.cash.AddFriendActivity" />
        <activity android:name="com.wyre.trade.stock.StocksActivity" />
        <activity android:name="com.wyre.trade.cash.AddBankActivity" />
        <activity android:name="com.wyre.trade.profile.ProfileActivity" />
        <activity android:name="com.wyre.trade.cash.CollectCashActivity" />
        <activity android:name="com.wyre.trade.cash.SendCashActivity" />
        <activity android:name="com.wyre.trade.cash.SendTargetActivity" />
        <activity android:name="com.wyre.trade.main.SupportActivity" />
        <activity android:name="com.wyre.trade.main.ChangePasswordActivity" />
        <activity android:name="com.wyre.trade.main.ResetPasswordActivity" />
        <activity android:name="com.wyre.trade.main.ForgotPasswordActivity" />
        <activity android:name="com.wyre.trade.stock.stockorder.StockOrderHistoryActivity" />
        <activity android:name="com.wyre.trade.chat.SocialGroupActivity" />
