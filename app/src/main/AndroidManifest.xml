        <activity android:name="com.wyre.trade.zabo.ZaboAccountActivity" />
        <activity android:name="com.wyre.trade.zabo.ZaboActivity">
            <intent-filter android:label="Zaho connect account">
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="www.joiintapp.com"
                    android:scheme="https" />
            </intent-filter>
        </activity>
        <activity android:name="com.wyre.trade.usdc.PaymentUserActivity" />
        <activity
            android:name="com.wyre.trade.main.SplashActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.wyre.trade.main.MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.wyre.trade.main.SignInActivity"
            android:parentActivityName="com.wyre.trade.main.MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.wyre.trade.main.SignUpActivity"
            android:parentActivityName="com.wyre.trade.main.MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.wyre.trade.home.HomeActivity"
            android:label="@string/title_activity_home"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.wyre.trade.stock.stocktrade.StocksTradingActivity" />
        <activity android:name="com.wyre.trade.stock.stocktrade.StockBuyActivity" />
        <activity android:name="com.wyre.trade.stock.stocktrade.StockSellActivity" />
        <activity android:name="com.wyre.trade.stock.stocktrade.StocksOrderActivity" />
