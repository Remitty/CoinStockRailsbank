<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.wyre.trade">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name="com.wyre.trade.MyApplication"
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.wyre.trade.usdc.PaymentUserListActivity"></activity>
        <activity android:name="com.wyre.trade.coins.StakeHistoryActivity" />
        <activity android:name="com.wyre.trade.stock.stockwithdraw.StockWithdrawHistoryActivity" />
        <activity android:name="com.wyre.trade.usdc.SendUsdcHistoryActivity" />
        <activity android:name="com.wyre.trade.stock.deposit.Coin2StockHistoryActivity" />
        <activity android:name="com.wyre.trade.stock.deposit.Bank2StockHistoryActivity" />
        <activity android:name="com.wyre.trade.coins.CoinWithdrawHistoryActivity" />
        <activity android:name="com.wyre.trade.coins.CoinSwapHistoryActivity" />
        <activity android:name="com.wyre.trade.token.TokenOrderBookActivity" />
